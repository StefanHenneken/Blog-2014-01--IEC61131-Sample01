<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.0.23">
  <POU Name="FB_Light" Id="{ac651da5-5748-4f92-a579-c38eff465f2e}">
    <Declaration><![CDATA[FUNCTION_BLOCK PUBLIC FB_Light IMPLEMENTS I_Light
VAR_INPUT
	bOn					:	BOOL;
	bOff				:	BOOL;
END_VAR
VAR_OUTPUT
	bControl			:	BOOL;
END_VAR
VAR
	rtrigOn				:	R_TRIG;
	rtrigOff			:	R_TRIG;
	_bControlLevel		:	BOOL;
END_VAR
]]></Declaration>
    <Implementation>
      <ST><![CDATA[rtrigOn(CLK := bOn);
IF (rtrigOn.Q) THEN
	M_On();
END_IF

rtrigOff(CLK := bOff);
IF (rtrigOff.Q) THEN
	M_Off();
END_IF

bControl := bControlLevel;
]]></ST>
    </Implementation>
    <Method Name="M_On" Id="{0b05d84c-3c99-44b7-a9f9-a9a6e37e56c6}">
      <Declaration><![CDATA[METHOD PUBLIC M_On
]]></Declaration>
      <Implementation>
        <ST><![CDATA[_bControlLevel := TRUE;]]></ST>
      </Implementation>
    </Method>
